extends layout.pug

block content
    h1 Books
    p
    a.button(href='/books/new') Create New Book
    table
        thead
            tr
            th Title
            th Author
            th Genre
            th Year
        tbody
            each book in books
                tr
                  td books.title
                  td books.author
                  td books.genre
                  td books.year

    